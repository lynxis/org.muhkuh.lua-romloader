# Switch to regexp syntax to get rooted patterns.
syntax: regexp

# Ignore the files generated by the build system.
^.sconsign.dblite$
^scons.bat$
^scons.sh$
^site_scons/site_init.py$

# Ignore all files in the targets folder.
^build/
^build_linux32/
^build_linux64/
^build_windows32/
^build_windows64/
^compile/
^plugins/romloader/_test/testcode/targets/
^plugins/romloader/machine_interface/netx/targets/
^plugins/romloader/uart/netx/targets/
^plugins/romloader/usb/netx/targets/


# Switch to glob syntax to get shell style patterns.
syntax: glob

# Ignore all compiled python modules.
*.pyc

# Ignore all backup files.
*~
